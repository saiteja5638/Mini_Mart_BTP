<mvc:View controllerName="uservaildation.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page"  showHeader="false" >
    
        <Toolbar id="_IDGenToolbar1" >
        <ToolbarSpacer id="_IDGenToolbarSpacer1" ></ToolbarSpacer>
    
        </Toolbar>
       
        <Panel id="_IDGenPanel1"  expandable="true" expand="true" expandAnimation="true" expanded="true"  >
        <VBox id="_IDGenVBox1" >
        <FlexBox id="_IDGenFlexBox1" justifyContent="Start"  >
        <Label id="_IDGenLabel1" text="User_Name" ></Label>
        <Input id="_IDGenInput1"  placeholder="12**" class="sapUiSmallMarginBegin"  ></Input>
        </FlexBox>
                <FlexBox id="_IDGenFlexBox2" >
        <Label id="_IDGenLabel2" text="User_Email" ></Label>
        <Input id="_IDGenInput2"  placeholder="12**" class="sapUiSmallMarginBegin" ></Input>
        </FlexBox>
                <FlexBox id="_IDGenFlexBox3" >
        <Label id="_IDGenLabel3" text="User_Contact" ></Label>
        <Input id="_IDGenInput3"  placeholder="12**" class="sapUiSmallMarginBegin" ></Input>
        </FlexBox>
                <FlexBox id="_IDGenFlexBox4" >
        <Label id="_IDGenLabel4" text="User_pass" ></Label>
        <Input id="_IDGenInput4"  placeholder="12**" class="sapUiSmallMarginBegin" ></Input>
        </FlexBox>
        <FlexBox id="_IDGenFlexBox5" >
        <Button id="_IDGenButton2"  text="Create"  press="onSubmit" ></Button>
        <Button id="_IDGenButton3"  text="Reset"  press="onReset" ></Button>
        </FlexBox>
        </VBox>
        </Panel>
        <Table id="_IDGenTable1"  items="{/User_Data}" mode="SingleSelectLeft"  >
        <infoToolbar>
        <Toolbar id="_IDGenToolbar2" >
        <ToolbarSpacer id="_IDGenToolbarSpacer2" ></ToolbarSpacer>
        <Button id="_IDGenButton4"  text="Update" type="Attention" press="setValues"  ></Button>
         <Button id="_IDGenButton5"  text="Delete" type="Critical" press="onDelete"  ></Button>
        </Toolbar>
        </infoToolbar>
        <columns>
        <Column id="_IDGenColumn1" >
        <Title id="_IDGenTitle1"  text="ID" ></Title>
        </Column>
                <Column id="_IDGenColumn2" >
        <Title id="_IDGenTitle2"  text="Name" ></Title>
        </Column>
                <Column id="_IDGenColumn3" >
        <Title id="_IDGenTitle3"  text="Email" ></Title>
        </Column>
                <Column id="_IDGenColumn4" >
        <Title id="_IDGenTitle4"  text="Contact" ></Title>
        </Column>
        </columns>
        <items>
        <ColumnListItem id="_IDGenColumnListItem1" >
        <Text id="_IDGenText1" text="{ID}" ></Text>
         <Text id="_IDGenText2" text="{USER_NAME}" ></Text>
          <Text id="_IDGenText3" text="{USER_EMAIL}" ></Text>
           <Text id="_IDGenText4" text="{USER_CONT}" ></Text>
        </ColumnListItem>
        </items>
        </Table>
    </Page>
</mvc:View>

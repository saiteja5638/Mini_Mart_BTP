<core:FragmentDefinition xmlns:table="sap.ui.table" xmlns:tnt="sap.tnt"   xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"  >
<Dialog  id="createTask"  title="Create Product"  titleAlignment="Center"  >
<VBox id="_IDGenVBox1" >
<FlexBox id="_IDGenFlexBox1"  direction="Column" class="sapUiTinyMargin"  >  
<Label id="_IDGenLabel1" text="PRODUCT NAME" ></Label>
<Input  width="100%"  id="pname"  ></Input>
</FlexBox>
<FlexBox id="_IDGenFlexBox2"  direction="Column" class="sapUiTinyMargin"  >  
<Label id="_IDGenLabel2" text="PRODUCT DESCRIPTION" ></Label>
<Input  width="100%" id="pdescr" ></Input>
</FlexBox>
<FlexBox id="_IDGenFlexBox3"  direction="Column" class="sapUiTinyMargin"  >  
<Label id="_IDGenLabel3" text="QUANTITY" ></Label>
<Input  width="100%" id="quantity" ></Input>
</FlexBox>
<FlexBox id="_IDGenFlexBox6"  direction="Column" class="sapUiTinyMargin"  >  
<Label id="_IDGenLabel6" text="AVATAR LINK" ></Label>
<Input  width="100%" id="avatar" ></Input>
</FlexBox>
</VBox>
<footer>
<Toolbar id="_IDGenToolbar1" >
<ToolbarSpacer id="_IDGenToolbarSpacer1" ></ToolbarSpacer>
<Button id="_IDGenButton1" text="Create" type="Emphasized" press="submit_create"  ></Button>
<Button id="_IDGenButton2"   text="close" type="Reject"  press="close_"  ></Button>
</Toolbar>
</footer>
</Dialog>
</core:FragmentDefinition>
<mvc:View xmlns:layout="sap.ui.layout" xmlns:uxap="sap.uxap" xmlns:cards="sap.f.cards" xmlns:f="sap.f" controllerName="productmaster.controller.View2"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    height="100%"
    xmlns="sap.m">
    <uxap:ObjectPageLayout id="_IDGenObjectPageLayout1" 
    enableLazyLoading="true"
		useIconTabBar="true"
		showTitleInHeaderContent="true"
		upperCaseAnchorBar="false"
    >
    <uxap:headerTitle>
    <uxap:ObjectPageDynamicHeaderTitle id="_IDGenObjectPageDynamicHeaderTitle1" >
    <uxap:expandedHeading>
    <HBox id="_IDGenHBox1" >
    <Title id="_IDGenTitle1"  text="Example1" wrapping="true" ></Title>
     <ObjectMarker id="_IDGenObjectMarker1"  type="Favorite"  class="sapUiTinyMarginBegin" ></ObjectMarker>
    </HBox>
    </uxap:expandedHeading>
    <uxap:expandedContent>
    <Text id="_IDGenText1" text="Example 2"/>
    </uxap:expandedContent>
    <uxap:actions>
    
     <Button id="_IDGenButton2" text="Edit" press="Edit_Quan" type="Emphasized" ></Button>
      <Button id="_IDGenButton1"   icon="sap-icon://decline" press="Close_tab" ></Button>
    </uxap:actions>
    </uxap:ObjectPageDynamicHeaderTitle>
    </uxap:headerTitle>
    <uxap:headerContent>
    <FlexBox id="_IDGenFlexBox1"  wrap="Wrap" >
    <Avatar id="_IDGenAvatar1"  class="sapUiSmallMarginEnd" displaySize="L"  src="https://buffer.com/library/content/images/size/w1200/2023/10/free-images.jpg" ></Avatar>
    <layout:VerticalLayout id="_IDGenVerticalLayout1"  class="sapUiSmallMarginBeginEnd" >
    <HBox id="_IDGenHBox2" >
    <Text id="_IDGenText2"   text="Current Dealer: " ></Text>
    <Text id="_IDGenText3"    ></Text>
    </HBox>
    <HBox id="_IDGenHBox" >
    <Text id="_IDGenText85"   text="Quantity Available:" ></Text>
    <Text id="_IDGenText856"    ></Text>
    </HBox>
    </layout:VerticalLayout>
    </FlexBox>
    </uxap:headerContent>
    <uxap:sections>
    <uxap:ObjectPageSection id="_IDGenObjectPageSection1"   title="Quantity Info" >
    <uxap:subSections>
    <uxap:ObjectPageSubSection id="_IDGenObjectPageSubSection1" >
    <Table id="_IDGenTable1"  items="{/items}" >
    <columns>
    <Column id="_IDGenColumn1"  hAlign="Center" >
    <Title id="_IDGenText4"  text="Branch" titleStyle="H6" ></Title>
    </Column>
        <Column id="_IDGenColumn2"  hAlign="Center" >
    <Title id="_IDGenText5"  text="Quantity(TON'S)" titleStyle="H6" ></Title>
    </Column>
        <Column id="_IDGenColumn3"  hAlign="Center" >
    <Title id="_IDGenText6"  text="Actuall Price(₹)" titleStyle="H6" ></Title>
    </Column>
        <Column id="_IDGenColumn4"  hAlign="Center" >
    <Title id="_IDGenText7"  text="Selling Price(₹)" titleStyle="H6" ></Title>
    </Column>
          <Column id="_IDGenColumn5"  hAlign="Center" >
    <Title id="_IDGenText8"  text="Margin" titleStyle="H6" ></Title>
    </Column>
    </columns>
    <items>
    <ColumnListItem id="_IDGenColumnListItem1" >
    <Title id="_IDGenText9" text="{BRANCH}" titleStyle="H6" ></Title>
    <Input id="_IDGenInput1" value="{QUAN_AVAIL}"  editable="false" width="50%" textAlign="Center" ></Input>
     <Input id="_IDGenInput2" value="{ACT_PRICE}"  editable="false" width="50%" textAlign="Center" ></Input>
      <Input id="_IDGenInput3" value="{SEL_PRICE}"  editable="false" width="50%" textAlign="Center" ></Input>
    <Text id="_IDGenText13" text="{MARGIN}" ></Text>
    </ColumnListItem>
    </items>
    </Table>
    </uxap:ObjectPageSubSection>
    </uxap:subSections>
    </uxap:ObjectPageSection>
    <uxap:ObjectPageSection id="_IDGenObjectPageSection2"  title="Dealers"  >
    <uxap:subSections>
    <uxap:ObjectPageSubSection id="_IDGenObjectPageSubSection2" >
      <Table id="_IDGenTable2" items="{/items}" >
    <columns>
    <Column id="_IDGenColumn6"  hAlign="Center" >
    <Text id="_IDGenText14"  text="Name"  ></Text>
    </Column>

        <Column id="_IDGenColumn7"  hAlign="Center" >
    <Text id="_IDGenText15"  text="Actuall Price($)"  ></Text>
    </Column>
        <Column id="_IDGenColumn8"  hAlign="Center" >
    <Text id="_IDGenText16"  text="Selling Price($)"  ></Text>
    </Column>
          <Column id="_IDGenColumn9"  hAlign="Center" >
    <Text id="_IDGenText17"  text="Margin"  ></Text>
    </Column>
            <Column id="_IDGenColumn10"  hAlign="Center" >
    <Text id="_IDGenText18"  text="Location"  ></Text>
    </Column>
    </columns>
    <items>
    <ColumnListItem id="_IDGenColumnListItem2" >
    <Text id="_IDGenText19" text="{NAME}" ></Text>
    <Text id="_IDGenText20" text="{PRICE}" ></Text>
    <Text id="_IDGenText21" text="{SEL_PRICE}" ></Text>
    <Text id="_IDGenText22" text="{MARGIN}" ></Text>
    <Text id="_IDGenText23" text="{LOCATIION}" ></Text>
    </ColumnListItem>
    </items>
    </Table>
    </uxap:ObjectPageSubSection>
    </uxap:subSections>
    </uxap:ObjectPageSection>
    </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View>
